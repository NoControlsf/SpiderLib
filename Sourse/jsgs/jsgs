CREATE TABLE `jsgs_link` (
  `area_name` varchar(200) DEFAULT NULL COMMENT '地区',
  `company_name` varchar(200) DEFAULT NULL COMMENT '公司名称',
  `company_link` varchar(200) DEFAULT NULL COMMENT '链接',
  `bt_time` varchar(200) DEFAULT NULL COMMENT '上传日期'
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='江苏国税链接';

CREATE TABLE `jsgs_detail` (
  `taxpayer_name` varchar(200) DEFAULT NULL COMMENT '纳税人名称',
  `taxpayer_code` varchar(200) DEFAULT NULL COMMENT '纳税人识别号',
  `organization_code` varchar(200) DEFAULT NULL COMMENT '组织机构代码',
  `registered_address` varchar(200) DEFAULT NULL COMMENT '注册地址',
  `legal_representative` varchar(200) DEFAULT NULL COMMENT '法定代表人或者负责人姓名、性别、证件名称及号码',
  `financial_officer` varchar(200) DEFAULT NULL COMMENT '负有直接责任的财务负责人姓名、性别、证件名称及号码',
  `agency` varchar(200) DEFAULT NULL COMMENT '负有直接责任的中介机构信息及其从业人员信息',
  `case_nature` varchar(200) DEFAULT NULL COMMENT '案件性质',
  `illegal_facts` varchar(200) DEFAULT NULL COMMENT '主要违法事实',
  `punishment_situation` varchar(200) DEFAULT NULL COMMENT '相关法律依据及税务处理处罚情况',
  `bt_time` varchar(200) DEFAULT NULL COMMENT '公布日期',
  `province` varchar(200) DEFAULT NULL COMMENT '所属省份',
  `city` varchar(200) DEFAULT NULL COMMENT '所属城市',
  `company_link` varchar(200) DEFAULT NULL COMMENT '链接'
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='江苏国税详情';

