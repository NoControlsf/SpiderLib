CREATE TABLE `company_profile` (
  `stock_code` varchar(200) DEFAULT NULL COMMENT '股票代码',
  `listed_category` varchar(200) DEFAULT NULL COMMENT '上市类别',
  `full_name` varchar(200) DEFAULT NULL COMMENT '公司全称',
  `english_name` varchar(200) DEFAULT NULL COMMENT '英文名称',
  `registered_address` varchar(200) DEFAULT NULL COMMENT '注册地址',
  `company_abbreviation` varchar(200) DEFAULT NULL COMMENT '公司简称',
  `legal_person` varchar(200) DEFAULT NULL COMMENT '法定代表人',
  `company_secretaries` varchar(200) DEFAULT NULL COMMENT '公司董秘',
  `registered_capital` varchar(200) DEFAULT NULL COMMENT '注册资本（万元）',
  `industry_category` varchar(200) DEFAULT NULL COMMENT '行业种类',
  `postal_code` varchar(200) DEFAULT NULL COMMENT '邮政编码',
  `company_telephone` varchar(200) DEFAULT NULL COMMENT '公司电话',
  `company_fax` varchar(200) DEFAULT NULL COMMENT '公司传真',
  `company_website` varchar(200) DEFAULT NULL COMMENT '公司网站',
  `time_to_market` VARCHAR (200) DEFAULT NULL COMMENT '上市时间',
  `stock_time` VARCHAR (200) DEFAULT NULL COMMENT '招股时间',
  `distribution_quantity` varchar(200) DEFAULT NULL COMMENT '发行数量（万股）',
  `issue_price` varchar(200) DEFAULT NULL COMMENT '发行价格（元）',
  `IPO` varchar(200) DEFAULT NULL COMMENT '发行市盈率（元）',
  `distribution_mode` varchar(200) DEFAULT NULL COMMENT '发行方式',
  `main_underwriter` varchar(100) DEFAULT NULL COMMENT '主承销商',
  `listed_recommendation` VARCHAR (200) DEFAULT NULL COMMENT '上市推荐人',
  `sponsor_institution` VARCHAR (200) DEFAULT NULL COMMENT '保荐机构'
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='公司概况';
